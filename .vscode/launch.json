    "version": "0.2.0",
    "configurations": [

        {
            "name": "family_finance (dev debug mode)",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "toolArgs": [
                "--dart-define-from-file",
                "env.json"
            ]
        },
        {
            "name": "family_finance (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "toolArgs": [
                "--dart-define-from-file",
                "lib/env.dev.json"
            ]
        },
        {
            "name": "family_finance (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "toolArgs": [
                "--dart-define-from-file",
                "lib/env.dev.json"
            ]
        }
    ]
}